this._attachEvent=function(g,h,f){if(!g){return;
}if(g.addEventListener){g.addEventListener(h,f,false);
}else{if(g.attachEvent){g.attachEvent("on"+h,f);
}else{switch(h){case"click":if(g.onclick){g.onclick=f;
}break;
case"mouseover":if(g.onmouseover){g.onmouseover=f;
}break;
case"mouseout":if(g.onmouseover){g.onmouseout=f;
}break;
}}}};
this._detachEvent=function(g,h,f){if(g.removeEventListener){g.removeEventListener(h,f,false);
}else{if(g.detachEvent){g.detachEvent("on"+h,f);
}else{switch(h){case"click":if(g.onclick){g.onclick=null;
}break;
case"mouseover":if(g.onmouseover){g.onmouseover=null;
}break;
case"mouseout":if(g.onmouseout){g.onmouseout=f;
}break;
}}}};
